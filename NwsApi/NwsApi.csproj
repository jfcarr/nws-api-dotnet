<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>NationalWeatherServiceAPI</PackageId>
    <Version>0.1.0</Version>
    <Authors>Jim Carr</Authors>
    <Title>Wrapper for the National Weather Service API</Title>
    <Description>
      This library provides a wrapper around a subset of calls to the
      National Weather Service API.
    </Description>
    <RepositoryUrl>https://github.com/jfcarr/nws-api-dotnet</RepositoryUrl>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

</Project>